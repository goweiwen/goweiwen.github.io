    style.
      body { background: #fff; font-family: 'Arimo', sans-serif; line-height: 1.3em; }
      main { background: #fff; max-width: 960px; margin: auto; padding: 3em; }
      header, section { max-width: 960px; }
      h1, h2, h3, .navbar { font-family: 'Cinzel', serif; letter-spacing: 0.05em; font-weight: 700; margin: 0; color: #396; }
      h2 { font-size: 1.5em; border-bottom: 1px dotted #606264; }
      body { color: #606264; }
      a { color: #60b0d0; text-decoration: underline; }

      * { transition: all 0.2s; }

      /* Navbar */
      main { margin-top: 3em; }
      .navbar { position: fixed; top: 0; left: 0; right: 0; z-index: 1; height: 3em; background-color: rgba(255,255,255,0.95); box-shadow: rgba(0,0,0,0.1) 0 2px 5px; }
      .navbar.is-hidden { transform: translateY(-110%); }
      .navbar .links { list-style: none; padding: 0; margin: 0; text-align: center; }
      .navbar .links li { display: inline-block; margin: 1em 0.5em 0 0.5em; height: 2em; text-transform: uppercase; }
      .navbar .links li a.link { color: #606264; text-decoration: none; padding: calc(1em - 6px); }
      .navbar .links li a:hover { border-bottom: 3px solid #339966; }
      .navbar .hamburger { cursor: pointer; display: none; position: absolute; top: 0; right: 0; padding: calc(1.5em - 12px); height: 24px; width: 24px; }
      .navbar .hamburger svg { height: 24px; width: 24px; vertical-align: middle; }
      @media (max-width: 920px) {
        .navbar .links { position: absolute; top: 0; visibility: hidden; opacity: 0; }
        .navbar .hamburger { display: block; }
        .navbar .links.is-visible { visibility: visible; opacity: 1; transform: translateY(3em); box-shadow: rgba(0,0,0,0.1) 0 2px 5px; }
      }

      /* Header */
      .header .me { display: flex; padding: 0 64px; }
      .header .me .face { margin-right: 5em; }
      .header .me .face img { border-radius: 100%; box-shadow: rgba(0,0,0,0.1) 0 2px 5px; width: 180px; height: 180px; }
      .header .me .face .contact { text-align: center; list-style: none; margin: 1em 0;; padding: 0; }
      .header .me .face .contact li { display: inline-block; }
      .header .me .face .contact li a { text-decoration: none; }
      .header .me .face .contact li .icon { width: 1.5em; height: 1.5em; vertical-align: middle; margin: 0 0.6em; }
      .header .me .face .contact li .icon:hover { transform: translateY(-1px); filter: drop-shadow(0px 2px 5px rgba(0,0,0,0.1)); }
      .header .me .face .contact li .icon:hover path { fill: #808488; }
      .download { margin: 2em 0; text-align: center; }
      .download a { text-decoration: none !important; display: inline-block; margin: 0 0.5em; }
      .download .button { display: inline-flex; align-items: center; padding: 0.5em 0.6em; line-height: 100%; background: #68c3a3; color: #fff; border: none; border-radius: 5px; box-shadow: rgba(0,0,0,0.1) 0 2px 5px; }
      .download .button:hover { cursor: pointer; transform: translateY(-1px); background: #4eb28f; box-shadow: rgba(0,0,0,0.1) 0 5px 8px; }
      .download .button .icon { width: 24px; height: 24px; margin-right: 0.5em; }

      section.contact ul { flex: initial; list-style: none; margin: 0; padding: 0; }
      section.contact ul li { display: block; }
      section.contact ul li a { text-decoration: none; }
      section.contact ul li .icon { width: 1em; height: 1em; vertical-align: middle; margin: 0 0.5em 0 1em; }

      /* Main */
      section { margin: 5em 0 0 0; overflow: hidden; }
      section ul { list-style: none; margin: 0; padding: 0; }
      section ul li { position: relative; margin: 0.3em 0; }
      section ul li:last-child { margin-bottom: 0; }

      /* Education, Experience */
      section header { display: flex; margin: 1em 0 0.3em 0; }
      section .company { flex: initial }
      section .title { flex: initial; }
      section .date { flex: auto; text-align: right; }

      section ul.description { list-style: disc; }
      section ul.description li { margin: 0 0 0 1.5em ; }

      /* Projects */
      .projects .logo { position: absolute; left: -80px; top: 0; width: 64px; height: 64px; }
      .projects li { margin-left: 80px; }

      /* Languages, Interests */
      .languages ul, .interests ul { margin: 8px 0; list-style: none; }
      .languages ul li, .interests ul li { margin: 0; }

      @media (max-width: 720px) {
        body { font-size: 11pt; }
        main { padding: 1em; }

        .header .me { flex-direction: column; padding: 0; }
        .header .me .face { margin: auto; }

        section header { flex-direction: column; }
        section .date { text-align: left; }

        .projects .logo { left: -40px; width: 32px; height: 32px; }
        .projects li { margin-left: 40px; }
      }

      .appear { animation: appear 1s; }
      @keyframes appear {
          from { opacity: 0; transform: translateY(30px); }
          to { opacity: 1; transform: translateY(0px); }
      }
