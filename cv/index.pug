    script.
      // Hamburger
      var links = document.querySelector('.links');
      document.querySelector('.hamburger').addEventListener('click', function(e) {
        e.preventDefault()
        links.classList.toggle('is-visible');
      });

      // Links
      document.querySelectorAll('.navbar .link').forEach(function(el) {
        var target = document.querySelector(el.getAttribute('href'));
        var y = target.offsetTop - 96;
        el.addEventListener('click', function(e) {
          e.preventDefault()
          window.scrollTo({ top: y, behavior: 'smooth' });
          links.classList.remove('is-visible');
        })
      });

      // Navbar visibility
      var navbar = document.querySelector('.navbar');
      var scrollPosition = window.pageYOffset;
      function handleNavbarVisibility() {
        if (scrollPosition >= window.pageYOffset) {
          navbar.classList.remove('is-hidden');
        } else {
          navbar.classList.add('is-hidden');
        }
      }
      window.addEventListener('scroll', handleNavbarVisibility);

      // Scroll Animation
      var nodeList = document.querySelectorAll('main > *:not(.appear)');
      function handleAppearAnimation() {
        scrollPosition = window.pageYOffset;
        nodeList.forEach(function(el) {
          if (el.offsetTop <= window.pageYOffset + window.innerHeight &&
              el.offsetTop + el.offsetHeight > window.pageYOffset) {
            nodeList = document.querySelectorAll('main > *:not(.appear)');
            el.classList.add('appear');
          }
        })
        if (nodeList.length === 0) {
          window.removeEventListener('scroll', handleAppearAnimation);
        }
      }
      window.addEventListener('scroll', handleAppearAnimation);
      handleAppearAnimation();