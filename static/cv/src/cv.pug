    title Goh Wei Wen
    link(href="css/normalize.css" rel="stylesheet")
    link(href="https://fonts.googleapis.com/css?family=Arimo:400,700|Cinzel:700" rel="stylesheet")
    style.
      body { background: #ffffff; font-family: 'Arimo', sans-serif; }
      main { background: #ffffff; max-width: 960px; margin: auto; padding: 1em; }
      header, section { max-width: 960px; }
      h1, h2, h3 { font-family: 'Cinzel', serif; letter-spacing: 0.05em; font-weight: 700; margin: 0; color: #339966; }
      h2 { font-size: 1.5em; border-bottom: 1px dotted #606264; }
      body { color: #606264; }
      a { color: #60b0d0; text-decoration: underline; }

      /* Header */
      .header { display: flex; }
      .header .face { border-radius: 100%; width: 8em; height: 8em; }
      .header .me { flex: auto; }
      .header .contact { flex: initial; list-style: none; margin: 0; padding: 0; }
      .header .contact li { display: block; }
      .header .contact li a { text-decoration: none; }
      .header .contact li .icon { width: 1em; height: 1em; vertical-align: middle; margin: 0 0.5em 0 1em; }

      /* Main */
      section { margin: 2em 0 0 0; }
      section ul { list-style: none; margin: 0; padding: 0; }
      section ul li { position: relative; margin: 0.3em 0; }
      section ul li:last-child { margin-bottom: 0; }

      /* Education, Experience */
      section header { display: flex; margin: 1em 0 0.3em 0; }
      section .company { flex: initial }
      section .title { flex: initial; }
      section .date { flex: auto; text-align: right; }

      section ul.description { list-style: disc; }
      section ul.description li { margin: 0 0 0 1.5em ; }

      /* Projects */
      .projects .logo { position: absolute; left: -40px; top: 0; width: 32px; height: 32px; }
      .projects li { margin-left: 40px; }

      /* Languages, Interests */
      .columns {
        display: flex;
        flex-flow: row wrap;
      }
      .columns ul {
        margin: 1em auto 1em 0;
        min-width: 160px;
      }
      .columns {
        margin-top: 0.2em;
      }
      .columns ul {
        margin: 0;
      }
      .columns ul li {
        display: inline;
      }

      .print-only { display: none; }

      @media print {
        body { font-size: 9.5pt; }
        body, a { color: #606264; }
        main { padding: 0; max-width: 720px; }
        header, section { max-width: 720px; }
        section { margin: 0.2em 0 0 0; }
        section header { margin: 0.3em 0 0 0; }
        h1 { font-size: 1.5em; }
        h2 { font-size: 1.0em; }
        .no-print { display: none; }
        .print-only { display: initial; }
      }

      @media screen and (min-width: 960px) {
        body { background-color: #f4f4f4; }
        main { margin: 1em auto; padding: 3em; box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2); }
      }
